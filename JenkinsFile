pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                sh 'npm install'
            }
        }
        stage('Deploy'){
            steps{
                nodejs(nodeJSInstallionName: 'nodejs'){
                    withAWS(credentials: 'aws-credentials'){
                        sh 'serverless deploy '
                    }
                }
            }
        }
    }
}